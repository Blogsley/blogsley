{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "~assets/*": [
                "src/assets/*"
            ],
            "~boot/*": [
                "src/boot/*"
            ],
            "~components/*": [
                "src/components/*"
            ],
            "~layouts/*": [
                "src/layouts/*"
            ],
            "~pages/*": [
                "src/pages/*"
            ],
            "~router/*": [
                "src/router/*"
            ],
            "~statics/*": [
                "src/statics/*"
            ],
            "~store/*": [
                "src/store/*"
            ],
            "~/*": [
                "src/*"
            ]
        }
    }
}